{
  "services": [
    {
      "id": "text-model",
      "name": "纯文本模型",
      "baseURL": "http://127.0.0.1:1234/v1",
      "model": "text-model-name",
      "apiKey": "NOT_NEEDED",
      "timeout": 1800000,
      "maxTokens": 4096,
      "capabilityTags": ["文本对话", "工具调用"],
      "capabilities": {
        "input": ["text"],
        "output": ["text", "structured_output", "tool_calling"]
      },
      "description": "纯文本对话模型，不支持多模态输入"
    },
    {
      "id": "vision-model",
      "name": "视觉理解模型",
      "baseURL": "http://127.0.0.1:1234/v1",
      "model": "vision-model-name",
      "apiKey": "NOT_NEEDED",
      "maxTokens": 2048,
      "capabilityTags": ["视觉理解", "图像分析", "多模态"],
      "capabilities": {
        "input": ["text", "vision"],
        "output": ["text", "structured_output"]
      },
      "description": "支持图片理解的视觉模型"
    },
    {
      "id": "multimodal-model",
      "name": "多模态模型",
      "baseURL": "http://127.0.0.1:1234/v1",
      "model": "multimodal-model-name",
      "apiKey": "NOT_NEEDED",
      "maxTokens": 4096,
      "capabilityTags": ["多模态", "视觉理解", "音频理解", "文件阅读"],
      "capabilities": {
        "input": ["text", "vision", "audio", "file"],
        "output": ["text", "structured_output", "tool_calling"]
      },
      "description": "支持多种输入类型的全能模型"
    },
    {
      "id": "coding-model",
      "name": "编程模型",
      "baseURL": "http://127.0.0.1:1234/v1",
      "model": "coding-model-name",
      "apiKey": "NOT_NEEDED",
      "maxTokens": 8192,
      "capabilityTags": ["编程", "代码生成", "代码审查"],
      "capabilities": {
        "input": ["text", "file"],
        "output": ["text", "structured_output", "tool_calling"]
      },
      "description": "专门用于代码生成和编程任务的模型，支持文件阅读"
    },
    {
      "id": "high-intelligence-model",
      "name": "高智能模型",
      "baseURL": "http://127.0.0.1:1234/v1",
      "model": "high-intelligence-model-name",
      "apiKey": "NOT_NEEDED",
      "maxTokens": 8192,
      "capabilityTags": ["高智能", "复杂推理", "深度分析"],
      "capabilities": {
        "input": ["text"],
        "output": ["text", "structured_output", "tool_calling"]
      },
      "description": "适用于复杂推理和深度分析任务的高智能模型"
    },
    {
      "id": "low-cost-model",
      "name": "低成本模型",
      "baseURL": "http://127.0.0.1:1234/v1",
      "model": "low-cost-model-name",
      "apiKey": "NOT_NEEDED",
      "maxTokens": 2048,
      "capabilityTags": ["低成本", "快速响应", "简单任务"],
      "capabilities": {
        "input": ["text"],
        "output": ["text"]
      },
      "description": "适用于简单任务的低成本快速模型"
    },
    {
      "id": "audio-model",
      "name": "音频模型",
      "baseURL": "http://127.0.0.1:1234/v1",
      "model": "audio-model-name",
      "apiKey": "NOT_NEEDED",
      "maxTokens": 4096,
      "capabilityTags": ["音频处理", "语音识别", "音频分析"],
      "capabilities": {
        "input": ["text", "audio"],
        "output": ["text", "structured_output"]
      },
      "description": "专门用于音频处理和语音识别的模型"
    }
  ]
}
